<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
<h1>Edit Your Note:</h1>
<%= form_for @note do |f| %>
  <p> Title:   <%= f.text_field :title %> </p>
  <p> Content: <%= f.text_area :content %> </p>
               <%= f.button :UpdateNote %>
  <% end %>
<%= link_to "Cancel", note_path %>
</div>
</div>
</div>
